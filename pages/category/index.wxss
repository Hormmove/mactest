page {
  height: 100%;
}
.cates {
  height: 100%;
}
.cates .cates.container {
  height: calc(100vh-90rpx);
  display: flex;
}
.cates .cates.container .left_menu {
  flex: 2;
}
.cates .cates.container .left_menu .menu_item {
  display: flex;
  height: 80rpx;
  justify-content: center;
  align-items: center;
  font-size: 30rpx;
  color: #000;
}
.cates .cates.container .left_menu .active {
  color: var(--themeColor);
  border-left: 5rpx solid currentColor;
}
.cates .cates.container .right_menu {
  flex: 5;
}
.cates .cates.container .right_menu .goods_group .goods_title {
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.cates .cates.container .right_menu .goods_group .goods_title .delimiter {
  color: #ccc;
  padding: 0 10rpx;
}
.cates .cates.container .right_menu .goods_group .goods_list {
  display: flex;
  flex-wrap: wrap;
}
.cates .cates.container .right_menu .goods_group .goods_list navigator {
  text-align: center;
  width: 33.33%;
}
